uint r_checkMaxImp(Critter& cr, Critter@ npc, int timeout) {
    int maxSpecialImp = 6;
    int maxCombatImp = 6;
    array<int> currentTotal = searchForImpPerks(cr);
     maxSpecialImp = maxSpecialImp - currentTotal[0];
     maxCombatImp = maxCombatImp - currentTotal[1];
     Log("maxSpecialImp: " + maxSpecialImp);
     Log("maxCombatImp: " + maxCombatImp );
	 array<int>  lim = {maxSpecialImp,maxCombatImp};
     searchForImpInv(cr,lim);
	 return DIALOG_END;
}

uint searchForImpInv(Critter& cr, array<int> limit) {
		uint ST = _CritCountItem(cr, PID_IMPLANT_STRENGTH);
		uint PE = _CritCountItem(cr, PID_IMPLANT_PERCEPTION);
		uint EN  = _CritCountItem(cr, PID_IMPLANT_ENDURANCE);
		uint CH =  _CritCountItem(cr, PID_IMPLANT_CHARISMA);
		uint IN = _CritCountItem(cr, PID_IMPLANT_INTELLIGENCE);
		uint AG = _CritCountItem(cr, PID_IMPLANT_AGILITY);
		uint LK = _CritCountItem(cr, PID_IMPLANT_LUCK);
        uint DER =  _CritCountItem(cr,PID_IMPLANT_DERMAL);
        uint PHO = _CritCountItem(cr, PID_IMPLANT_PHOENIX);
        uint NEN = _CritCountItem(cr,PID_IMPLANT_NEMEAN);

        int currentSP = limit[0];
        int currentCB = limit[1];
        int currentInvSP = ST + PE + EN + CH + IN + AG + LK;
        int currentInvCB = DER + PHO + NEN;

        if( currentSP - currentInvSP < 0 ) {
            return DIALOG_END;
        };
		
        array<int>  impSP = {
        ST,
        PE,
        EN,
        CH,
        IN,
        AG,
        LK
        };
        for (int i = 0; i < impSP.length(); i++)
        {
           currentSP =  currentSP - impSP[i] ;
        };
        
			if( 7 > currentSP) {
            Log("en SP LOOP");
            int index = 0;
            for(uint i = ST_STRENGTH; i <= ST_LUCK; i++) {
                   if(impSP[index] > 0) {
                    cr.StatBase[i] += impSP[index];
                    index++;
                   }
                    
            }
        }
        return DIALOG_END;
}

array<int> searchForImpPerks(Critter& cr) {
    int currentcantSpecial =  cr.PerkBase[469] + cr.PerkBase[468] + cr.PerkBase[467] + cr.PerkBase[466] + cr.PerkBase[465] + cr.PerkBase[464] + cr.PerkBase[463];
    int currentcantCombat =    cr.PerkBase[433] + cr.PerkBase[432] + cr.PerkBase[434] + cr.PerkBase[435] + cr.PerkBase[462];
	array<int> allImp = { currentcantSpecial, currentcantCombat };
    return allImp;
}

uint dlg_SayBuyMedGel(Critter& player, Critter@ npc, string@ text)
{
    if(!IS_DIALOG_SAY_MODE(text))
        return DIALOG_END;

    uint price = 25;
    uint cnt = 0;
    if(!StrToInt(text, cnt))
        return DIALOG_END;
        
    if(cnt > 0)
    {
        if(_CritCountItem(player, PID_BOTTLE_CAPS) >= price*cnt)
        {
            _CritDeleteItem(player, PID_BOTTLE_CAPS, (price*cnt));
            player.AddItem(PID_BIO_GEL, cnt);
            npc.Say(SAY_NORM_ON_HEAD, "Pleasure doing business.");
        }
        else
        {
            npc.Say(SAY_NORM_ON_HEAD, "Come back with money!");
            return DIALOG_END;
        }
    }
    return DIALOG_END;
}

