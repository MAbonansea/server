void setBonusItemArmor(Item@ it)
{
    array<uint> allBonus = {
        Random(0,1),
        Random(0,1),
        Random(0,1),
        Random(0,1),
        Random(0,1),
        Random(0,1),
    };
    array<uint> bonusType = {
        BONUS_ARMOR_CRIT_CHANCE,
        BONUS_ARMOR_CRIT_POWER,
        BONUS_ARMOR_NORMAL_DT,
        BONUS_ARMOR_LASER_DT,
        BONUS_ARMOR_FIRE_DT,
        BONUS_ARMOR_PLASMA_DT,
        BONUS_ARMOR_NORMAL_DR,
        BONUS_ARMOR_LASER_DR,
        BONUS_ARMOR_FIRE_DR,
        BONUS_ARMOR_PLASMA_DR,
        BONUS_ARMOR_EXPLODE_DT,
        BONUS_ARMOR_EXPLODE_DR,
        BONUS_ARMOR_MAX_AP,
        BONUS_ARMOR_STRENGTH,
        BONUS_ARMOR_PERCEPTION,
        BONUS_ARMOR_ENDURANCE,
        BONUS_ARMOR_CHARISMA,
        BONUS_ARMOR_INTELLIGENCE,
        BONUS_ARMOR_AGILITY,
        BONUS_ARMOR_LUCK
    };
    
     for(uint b = 0; b < allBonus.length(); b++) {
        uint bonus = chosenBonus();
		bool itemAlreadyHasBonus = false;
		if (b == 0) {
		setBonusItemArmor(allBonus[b],bonus, it, b);
		} else {
        itemAlreadyHasBonus = HasBonusAlready(it, bonus);

		}
        while (itemAlreadyHasBonus == false && b > 0) {
            setBonusItemArmor(allBonus[b],bonus, it, b);
        };
       };
}

uint chosenBonus() {
    return Random(0,19);

}

// Checks if there is no such bonus already on an item
bool HasBonusAlready(Item@ it, int bonusType)
{
	if(it.Val0==bonusType) return true;
	if(it.Val1==bonusType) return true;
	if(it.Val2==bonusType) return true;
	if(it.Val3==bonusType) return true;
	if(it.Val4==bonusType) return true;
	return false;
}

void setBonusItemArmor(uint rand, uint bonusType, Item@ it, uint index) {
    if(rand == 0) {
        return;
    }
    int minRoll=1;
	int maxRoll=1;

    // Choose bonus roll
	int roll = Random(minRoll, maxRoll);
	
	// Choose which value
	switch(index)
	{
		case 0: {it.Val0=bonusType; it.Val5=roll;break;}
		case 1: {it.Val1=bonusType; it.Val6=roll;break;}
		case 2: {it.Val2=bonusType; it.Val7=roll;break;}
		case 3: {it.Val3=bonusType; it.Val8=roll;break;}
		case 4: {it.Val4=bonusType; it.Val9=roll;break;}
		default: break;
	}
	Log("Armor bonus: " + bonusType + " Value: " + roll);
	it.Update();
return;
   
}

void setOneByOneBonusWeapon(uint rand, Item@ it, uint index)
{
    if(rand == 0 ) {
        return;
    }
	uint minRoll=1;
	uint maxRoll=1;
	array<uint> bonusType = { 
	BONUS_WEAPON_CRITICAL_ROLL, //'BONUS_WEAPON_CRITICAL_ROLL'
	BONUS_WEAPON_CRITICAL_CHANCE, //'BONUS_WEAPON_CRITICAL_CHANCE'
