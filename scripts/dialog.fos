void revertOtherShape( Critter@ player) {
        player.StatBase[ST_BASE_CRTYPE ] = 17;
        player.StatBase[ST_OVERRIDE_CRTYPE] = 17;
        player.StatBase[ST_BODY_TYPE] = 10;

    if(player.StatBase[MODE_ROBOT]  == 1) { return;}
    
        if(player.StatBase[MODE_ROBOT]  == 2 ) {
        player.StatBase[ST_LUCK_EXT] +=4;
