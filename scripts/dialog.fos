	BONUS_WEAPON_MAX_RANGE, //'BONUS_WEAPON_MAX_RANGE'
	};
	
    	switch(index)
	{
		case 0: {maxRoll=5;break;}
		case 1: {maxRoll=5;break;}
		case 2: {minRoll=10;maxRoll=25;break;}
		case 3: {minRoll=10;maxRoll=25;break;}
		case 4: {minRoll=5;maxRoll=25;break;}
		case 5: {minRoll=1;maxRoll=2;break;}
		default: break;
	}



        // Choose bonus roll
	    uint roll = Random(minRoll, maxRoll);
	
	    // Choose which value
	    switch(index)
	{
		case 1: {it.Val0=bonusType[index]; it.Val5=roll;break;}
		case 2: {it.Val1=bonusType[index]; it.Val6=roll;break;}
		case 3: {it.Val2=bonusType[index]; it.Val7=roll;break;}
		case 4: {it.Val3=bonusType[index]; it.Val8=roll;break;}
		case 5: {it.Val4=bonusType[index]; it.Val9=roll;break;}
		default: break;
	}
	    Log("Weapon bonus: " + bonusType[index] + " Value: " + roll);
	    it.Update();

}



